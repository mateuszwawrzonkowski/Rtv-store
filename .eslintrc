{
  "extends": [
      "react-app",
      "airbnb",
      "airbnb/hooks"
  ],
  "rules": {
      "react/jsx-filename-extension": [
          1,
          {
              "extensions": [".js", ".jsx"]
          }
      ],
      "import/prefer-default-export": 0,
      "import/no-extraneous-dependencies": [
          "error",
          {
              "devDependencies": true
          }
      ],
      "linebreak-style": 0,
      "no-shadow": "off",
      "no-param-reassign": 0
  },
  "settings": {
      "import/resolver": {
          "node": {
              "paths": ["src"]
          }
      }
  }
}